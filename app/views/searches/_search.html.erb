<% if user_signed_in? %>
  <%= form_with url: search_path, method: :get, local: true do |s| %>
    <%= s.text_field :content %>
    <%= s.select :model, options_for_select({"User" => "user","Book" => "book"}) %>
    <%= s.select :method, options_for_select({"完全一致" => "perfect","前方一致" => "forward","後方一致" => "backward","部分一致" => "partial"}) %>

    <%= s.submit "検索"%>
    <%= %>
  <% end %>
<% end %>